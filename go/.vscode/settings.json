{
    "go.gopath": "${workspaceRoot}/.go",
    "go.testOnSave": true,
    "go.testFlags": [
        "-bench",
        "."
    ],
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/**": true,
        "**/.go/**": true,
    },
    "editor.renderWhitespace": "all"
}